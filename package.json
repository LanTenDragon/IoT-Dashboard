{
  "name": "iot-dashboard",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "test": "mocha",
  "scripts": {
    "start": "npm run pack & npm run monitor",
    "monitor": "cross-env NODE_ENV=development nodemon src/server.js",
    "pack": "webpack src/index.js --output public/js/index.js -d --watch",
    "build": "webpack src/index.js --output public/js/index.js -p",
    "test": "cross-env NODE_ENV=test ospec  --preload src/test-setup.js",
    "production": "cross-env NODE_ENV=production npm run-script build && node src/server.js",
    "webpack": "webpack --output public/js/index.js -d --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mithril": "^2.0.4",
    "mithril-dnd": "^1.1.3",
    "standard": "^14.3.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "jsdom": "^16.2.2",
    "mithril-query": "^3.1.0",
    "nodemon": "^2.0.4",
    "ospec": "^4.1.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
